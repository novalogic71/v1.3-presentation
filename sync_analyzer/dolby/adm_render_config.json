{
    "$schema": "../data/schemas/config.schema.json",
    "version": 0,
    "processes": [
        {
            "name": "input",
            "type": "read_adm_bw64",
            "out_ports": ["out_axml"]
        },
        {
            "name": "drop_divergence",
            "type": "drop_blockformat_subelements",
            "parameters": {
                "objects_subelements": ["ObjectDivergence"]
            },
            "in_ports": ["in_axml"],
            "out_ports": ["out_axml"]
        },
        {
            "name": "fix_ds_frequency",
            "type": "fix_ds_frequency",
            "in_ports": ["in_axml"],
            "out_ports": ["out_axml"]
        },
        {
            "name": "fix_block_durations",
            "type": "fix_block_durations",
            "in_ports": ["in_axml"],
            "out_ports": ["out_axml"]
        },
        {
            "name": "fix_stream_pack_refs",
            "type": "fix_stream_pack_refs",
            "in_ports": ["in_axml"],
            "out_ports": ["out_axml"]
        },
        {
            "name": "rewrite_objects",
            "type": "rewrite_content_objects_emission",
            "in_ports": ["in_axml"],
            "out_ports": ["out_axml"]
        },
        {
            "name": "render",
            "type": "render",
            "parameters": {
                "layout": "0+2+0"
            },
            "in_ports": ["in_axml"],
            "out_ports": ["out_samples"]
        },
        {
            "name": "output",
            "type": "write_bw64",
            "in_ports": ["in_samples"]
        }
    ],
    "connections": [
        ["input.out_samples", "render.in_samples"]
    ]
}
